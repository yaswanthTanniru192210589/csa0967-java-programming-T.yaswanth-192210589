public class Mal {
    int number;
    int n;
    public Mal(int number) {
  this.number = number;
this.n=n;
    }
    public int malcal() {
        for (int i = 1; i <= n; i++) { 
            System.out.println(number+"*"+i+"="+(number*i));
        }
    }
    public static void main(String[] args) {
Scanner  reader=new Scanner(System.in);
int number=reader.nextInt();
int n=reader.nextInt();
        Mal m1 = new Mal(number,n);
    }
}
