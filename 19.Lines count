import java.util.Scanner;
public class linescount{
public static void main(String[] args){
Scanner reader=new Scanner(System.in);
String n="";
int a;
int count=0,flag=0,k=0;
while(k==0){
n=reader.nextLine();
a=n.length();
count++;
for(int i=0;i<a;i++){
char ch=n.charAt(i);
if(ch=='0'){
k++;
break;
}
}
}
count=count-1;
System.out.println("the lines contains: "+count);
}
}
